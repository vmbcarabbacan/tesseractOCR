{
    "name": "vmbcarabbacan/teseract-ocr",
    "description": "Extract text image and pdf file",
    "type": "library",
    "keywords": ["Tesseract", "OCR", "text recognition", "eid recognition"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vmbcarabbacan\\TeseractOcr\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vincent Mark Bondoc Carabbacan",
            "email": "vmbcarabbacan@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2|^8.0",
        "ext-imagick": "3.6.0",
        "spatie/pdf-to-image": "^2.3",
        "symfony/process": "5.4.*",
        "thiagoalessio/tesseract_ocr": "^2.13"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
